@{
    ViewData["Title"] = "Informe Eventos";
}
<link rel="stylesheet" href="~/css/Socios.css" asp-append-version="true" />

<div class="p-3">
    @*TABLA*@
    <div class="mt-2" style="height: 100%;">
        <div class="card shadow-sm">
            <div class="card-header row m-0 p-2">
                <div class="col-md-8 d-flex align-items-center">
                     <button class="btn btn-primary boton-imprimir" onclick="Imprimir()"><i class="fa-solid fa-print"></i></button>
                </div>
                <div class="col-md-4 text-end d-flex align-items-center justify-content-end">
                    <h3 class="m-0"><i class="fa-solid fa-table pe-2"></i>INFORME EVENTOS</h3>
                </div>
            </div>
            <div class="card-body mb-5">
                <div class="table-responsive" style="height: 450px; overflow-y: auto;">
                    <table id="tabla-imprimir" class="table table-hover align-middle">
                        <thead>
                            <tr>
                                <th>TIPO DE EVENTO</th>
                                <th>EVENTO</th>
                            </tr>
                        </thead>
                        <tbody id="tbody-InformeEventos">
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>


    @section Scripts {
  <script src="~/js/InformeEventos.js"></script>
}